    'Core',
    'RCTActionSheet',
    'RCTCameraRoll',
    'RCTGeolocation',
    'RCTImage',
    'RCTLinkingIOS',
    'RCTNetwork',
    'RCTPushNotification',
    'RCTSettings',
    'RCTText',
    'RCTVibration',
    'RCTWebSocket',
  ]
  pod 'react-native-linkedin-login', :path => '../node_modules/react-native-linkedin-login'
end

target 'RNLinkedinLoginExample' do
  available_pods
end

target 'RNLinkedinLoginExampleTests' do
    available_pods
end


